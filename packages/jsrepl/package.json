{
  "name": "jsrepl",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "NUXT_PREPARE=1 nuxt prepare",
    "lint": "nuxi typecheck && eslint .",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:playwright": "playwright test",
    "test:playwright:web-server:build": "nuxt build --dotenv .env.test",
    "test:playwright:web-server:start": "pnpm supabase:start && nuxt preview --dotenv .env.test",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:restart": "pnpm supabase:stop && pnpm supabase:start",
    "supabase:reset": "supabase db reset",
    "supabase:link": "supabase link",
    "supabase:generate-types": "supabase gen types --lang=typescript --local --schema public > types/database.types.ts",
    "supabase:generate-migration": "supabase db diff | supabase migration new",
    "supabase:generate-seed": "supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull"
  },
  "dependencies": {
