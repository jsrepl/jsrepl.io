    "clsx": "^2.1.1",
    "debounce": "^2.1.0",
    "fflate": "^0.8.2",
    "interactjs": "^1.10.27",
    "lucide-vue-next": "^0.408.0",
    "monaco-editor": "^0.50.0",
    "monaco-themes": "^0.4.4",
    "radix-vue": "^1.9.3",
    "shiki": "^1.11.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4"
