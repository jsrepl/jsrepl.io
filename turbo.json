{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_PREVIEW_URL",
        "NEXT_PUBLIC_APP_VERSION",
        "JSREPL_ORIGIN",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "VERCEL_ENV",
        "VERCEL_BRANCH_URL",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "VERCEL_URL"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "preview": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {
      "dependsOn": ["test:vitest", "test:playwright"]
    },
    "test:vitest": {},
    "test:playwright": {
      "dependsOn": ["test:playwright:web-server:build"]
    },
    "test:playwright:web-server:build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_PREVIEW_URL",
        "NEXT_PUBLIC_APP_VERSION",
        "JSREPL_ORIGIN",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "VERCEL_ENV",
        "VERCEL_BRANCH_URL",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "VERCEL_URL"
      ]
    },
    "@jsrepl/preview-entry#test:playwright:web-server:build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["../jsrepl/public/e.js"],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_PREVIEW_URL",
        "NEXT_PUBLIC_APP_VERSION",
        "JSREPL_ORIGIN",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "VERCEL_ENV",
        "VERCEL_BRANCH_URL",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "VERCEL_URL"
      ]
    },
    "test:playwright:web-server:start": {
      "cache": false,
      "persistent": true,
      "interactive": false
    },
    "test:vitest:watch": {
      "cache": false,
      "persistent": true
    },
    "@jsrepl/jsrepl#build": {
      "dependsOn": ["@jsrepl/preview-entry#build"]
    },
    "@jsrepl/jsrepl#test:playwright:web-server:build": {
      "dependsOn": ["@jsrepl/preview-entry#test:playwright:web-server:build"]
    }
  },
  "envMode": "loose"
}
